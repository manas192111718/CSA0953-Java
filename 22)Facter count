import java.util.Scanner;
public class FactorCounter {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int factors = 0;
        for (int i = 1; i <= n; i++) {
            if (n % i == 0) {
                factors++;
            }
        }
        System.out.print("Number of factors = " + factors);
        input.close();
    }
}
