import java.util.Scanner;
public class SquareRootCalculator {
    public static void main(String args[]) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter a perfect square number: ");
        double n = input.nextDouble();
        if (n >= 0) {
            double sqrtPositive = Math.sqrt(n);
            double sqrtNegative = -Math.sqrt(n);
            System.out.println("Positive Square Root: " + sqrtPositive);
            System.out.println("Negative Square Root: " + sqrtNegative);
        } else {
            System.out.println("Please enter a non-negative number.");
        }
        input.close();
    }
}
